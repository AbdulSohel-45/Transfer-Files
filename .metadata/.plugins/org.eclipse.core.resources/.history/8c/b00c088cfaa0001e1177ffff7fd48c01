import java.sql.SQLException;

public class DBDemo
{
	public static int getDB(String firstName,int number,String source,String destination,String vehilce,int distance,int price ) throws ClassNotFoundException, SQLException
	{
		String url="jdbc:mysql://localhost:3306/offo";
		String user="root";
		String pwd="root";
		Class.forName("com.mysql.cj.jdbc.Driver");
		Connection con=DriverManager.getConnection(url,user,pwd);
		PreparedStatement pst=con.prepareStatement("insert into app values(?,?,?,?,?,?,?");
		
		return 0;
	}
}
